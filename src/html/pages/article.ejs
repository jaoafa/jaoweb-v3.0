<%
  var articleMeta = {
    title: "jao鯖Webサイトリニューアルのお知らせ",
    pagetype: "blog"
  };
-%>
<%- include( '../components/_header.ejs', { meta: articleMeta } ) -%>
<%- include( '../components/_breadcrumb.ejs', { meta: articleMeta } ) -%>
    <div class="l-main__article">
      <article class="l-article">
        <header class="l-article__header">
          <div class="p-articleHeader">
            <div class="p-articleHeader__eyecatch">
              <img class="p-articleHeader__image" src="./img/sample/mc06.png" alt="eyecatch" />
            </div>
            <div class="p-articleHeader__info">
              <h1 class="p-articleHeader__title">
                <%= articleMeta.title %>
              </h1>
              <ul class="p-articleHeader__meta">
                <li class="p-articleHeader__metaItem">
                  <a href="">
                    Hiratake
                  </a>
                </li>
                <li class="p-articleHeader__metaItem">
                  <a href="">
                    2019/09/08
                  </a>
                </li>
                <li class="p-articleHeader__metaItem">
                  <a href="">
                    お知らせ
                  </a>
                </li>
              </ul>
            </div>
          </div>
        </header>
        <div class="l-article__main">
          <div class="l-article__content">
            <div class="p-articleContent">
              
            </div>
          </div>
          <div class="l-article__share">
            <div class="p-articleShare">
              <a class="p-articleShare__button p-articleShare__button--twitter" href="">
                <svg class="p-articleShare__icon">
                  <use xlink:href="./img/brand.svg#ico_twitter"></use>
                </svg>
              </a>
              <a class="p-articleShare__button p-articleShare__button--facebook" href="">
                <svg class="p-articleShare__icon">
                  <use xlink:href="./img/brand.svg#ico_facebook"></use>
                </svg>
              </a>
              <a class="p-articleShare__button p-articleShare__button--hatena" href="">
                <svg class="p-articleShare__icon">
                  <use xlink:href="./img/brand.svg#ico_hatenabookmark"></use>
                </svg>
              </a>
              <a class="p-articleShare__button p-articleShare__button--line" href="">
                <svg class="p-articleShare__icon">
                  <use xlink:href="./img/brand.svg#ico_line"></use>
                </svg>
              </a>
            </div>
          </div>
        </div>
      </article>
    </div>
<%- include( '../components/_footer.ejs' ) -%>